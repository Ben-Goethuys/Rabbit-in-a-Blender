CREATE OR REPLACE TABLE {{project_id}}.{{dataset_id_work}}.{{omop_table}}__{{concept_id_column}}_usagi
CLUSTER BY sourceCode
AS
SELECT 
    sourceCode,
    sourceName,
    mappingStatus,
    CAST(IF(conceptId = '','0',conceptId) AS INT64) conceptId,
    conceptName,
    domainId
FROM {{project_id}}.{{dataset_id_work}}.{{omop_table}}__{{concept_id_column}}_usagi